<%= simple_form_for(@resource) do |f| %>
  <%= f.input :name %>
  <%= f.input :website %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :email %>
  <%= f.input :what_they_do %>
  <%= f.input :about %>
  <%= f.input :tip %>
  <%= f.input :taglines_as_string %>
  <%= f.submit %>
<% end %>
