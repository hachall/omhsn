
<div class="container form-center">
  <%= simple_form_for(@resource) do |f| %>
    <%= f.input :name %>
    <%= f.input :website %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :email %>
    <%= f.input :what_they_do %>
    <%= f.input :category, collection: ['National Health Service', 'Peer Support Groups', 'Self-Help and Apps', 'Helplines', 'Students', 'Support Services', 'Private Medical Practices', 'Emergency'] %>
    <%= f.input :about %>
    <%= f.input :tip %>
    <%= f.input :taglines_as_string %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit %>
  <% end %>
</div>

