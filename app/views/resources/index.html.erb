
<form action="/resources" method="get" class="flex-form form-inline search-index">
  <input type="text" name="query" class="form-control input-index text-center" style="vertical-align: baseline" placeholder="What are you looking for?">
  <button type="submit" class="search-button">
    <i class="fa fa-search"></i>
  </button>
</form>

<% @resources.each do |resource| %>
<h1><%= resource.name %></h1>
<% end %>

<div
  id="map"="
  style="width: 100%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>"
></div>
