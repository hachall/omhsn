<div class="container form-center">
  <%= simple_form_for(@event) do |f| %>
    <%= f.input :name %>
    <%= f.input :date %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :card_description %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit %>
  <% end %>
</div>



<%= content_for :after_js do %>
  <%= javascript_pack_tag 'autocomplete' %>
<% end %>
