<div class="marker-card text-center">
  <%= link_to event_path(event) do %>
    <% url = event.photo.nil? ? 'andycat.png' : event.photo %>
    <%= cl_image_tag url, width: 200, height:180, class: "marker-photo" %>
    <h2><%= event.name.capitalize %></h2>
    <p><%= event.address%></p>
  <% end %>
</div>

